<?xml version="1.0" encoding="UTF-8"?>
<Configuration status="WARN">
  <Appenders>
    <Console name="CONSOLE" target="SYSTEM_OUT">
      <PatternLayout pattern="%d{HH:mm:ss.SSS} %-5level [%-4.4t] %-30.30C{4} - %msg%n" />
    </Console>
  </Appenders>
  <Loggers>
    <Logger name="jfortune" level="INFO"/>
    <Root level="WARN">
      <AppenderRef ref="CONSOLE" />
    </Root>
  </Loggers>
</Configuration>
